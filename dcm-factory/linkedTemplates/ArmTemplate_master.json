{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dcm-factory"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"LS_DCM_MONGO_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LS_DCM_MONGO'"
		},
		"dcm cosmos db_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'dcm cosmos db'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"DCMAzureBlobStorage_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DCMAzureBlobStorage'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devdcmstorage.dfs.core.windows.net"
		},
		"LS_DCM_MONGO_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dcm"
		},
		"dcm cosmos db_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dcm"
		},
		"Begin SQL Injection_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/b5bfc7e0-0306-4f8a-aacc-cefe8ebd78e8/resourceGroups/Datacapture/providers/Microsoft.Storage/storageAccounts/devdcmstorage"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dcm-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"LS_DCM_MONGO_connectionString": {
						"value": "[parameters('LS_DCM_MONGO_connectionString')]"
					},
					"dcm cosmos db_connectionString": {
						"value": "[parameters('dcm cosmos db_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"DCMAzureBlobStorage_connectionString": {
						"value": "[parameters('DCMAzureBlobStorage_connectionString')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"LS_DCM_MONGO_properties_typeProperties_database": {
						"value": "[parameters('LS_DCM_MONGO_properties_typeProperties_database')]"
					},
					"dcm cosmos db_properties_typeProperties_database": {
						"value": "[parameters('dcm cosmos db_properties_typeProperties_database')]"
					},
					"Begin SQL Injection_properties_typeProperties_scope": {
						"value": "[parameters('Begin SQL Injection_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}